    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "test": "cd backend && node test-server.js"
  },
  "keywords": [
    "playlist",
    "comparator",
    "spotify",
    "apple-music",
    "music",
    "react",
    "nodejs"
  ],
