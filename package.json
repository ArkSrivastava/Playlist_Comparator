{
  "name": "playlist-comparator",
  "version": "1.0.0",
  "description": "Cross-Platform Playlist Comparator - Compare playlists from Spotify, Apple Music, and more",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
